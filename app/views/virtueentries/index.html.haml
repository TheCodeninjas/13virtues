%h2 Entries
%table.tb
	%thead
		%tr
			%th Virtue#
			%th Date
			%th Violations
			%th Notes
	%tbody
		-if @entries
			-@entries.each do |entry|
				%tr
					%td= entry.v_id
					%td= entry.date
					%td= entry.violation_count
					-if entry.note
						%td= entry.note.note
					-else
						%td

=link_to 'Create Entry', new_classroom_virtueentry_path
